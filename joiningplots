import sys
from PIL import Image

list_im = ['20170620-172830m1.png','20170628-182557m1.png']
new_im = Image.new('RGB', (2000,2000)) #creates a new empty image, RGB mode, and size 444 by 95

for elem in list_im:
    for i in xrange(0,2000,2000):
        im=Image.open(elem)
        new_im.paste(im, (i,0))
new_im.save('m1compaision.png')
